<Window x:Class="SpeakUP_1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SpeakUP_1"
        mc:Ignorable="d"
        Title="MainWindow" 
        ResizeMode="NoResize"
        Height="565" Width="971">

    <Grid x:Name="W" Width="971" Background="#FFF7F7F7" >


        <Image x:Name="LoginB" Margin="729,10,0,0" Source="/Component 1.png" Stretch="UniformToFill" MouseUp="LoginB_MouseUp" HorizontalAlignment="Left" Width="222" Height="80" VerticalAlignment="Top"/>
        <Image x:Name="STOP" Margin="11,0,0,10" Source="/Component 1 (6).png" Stretch="Uniform" MouseUp="STOP_MouseUp"  HorizontalAlignment="Left" VerticalAlignment="Bottom" Height="124" Width="315"/>


        <Image x:Name="REC" Margin="10,0,0,10" Source="/REC.png" Stretch="UniformToFill" MouseUp="REC_MouseUp" HorizontalAlignment="Left" VerticalAlignment="Bottom" Height="124" Width="315" />
        <Image Margin="8,10,0,226" Source="/Component 1 (9).png" Stretch="Fill" HorizontalAlignment="Left" Width="315" />
        <Image x:Name="File" Margin="5,0,0,142" Source="/222 (1).png" Stretch="UniformToFill" HorizontalAlignment="Left" VerticalAlignment="Bottom" Height="80" Width="320" MouseUp="File_MouseUp"/>





    </Grid>
</Window>
